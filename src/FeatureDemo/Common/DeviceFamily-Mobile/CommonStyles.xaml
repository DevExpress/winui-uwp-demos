                                                        <Thumb x:Name="HorizontalThumb" Background="{StaticResource MainBrush}" Style="{StaticResource SliderThumbStyle}" DataContext="{TemplateBinding Value}" Height="8" Width="24" Grid.Row="0" Grid.RowSpan="3" Grid.Column="1" AutomationProperties.AccessibilityView="Raw" />
