                        <TextBlock Text="Options" Style="{StaticResource OptionsTextStyle}" Grid.Row="3"/>
                        <ContentPresenter Margin="20, 0, 20, 0" Grid.Row="4"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
