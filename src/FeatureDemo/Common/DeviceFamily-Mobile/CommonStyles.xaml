                            <Style TargetType="Editors:SpinEdit">
                                <Setter Property="MinWidth" Value="{ThemeResource SpinEditOptionThemeMinWidth}"/>
                                <Setter Property="MinHeight" Value="{ThemeResource SpinEditOptionThemeMinHeight}"/>
                                <Setter Property="Foreground" Value="{ThemeResource TextBoxOptionForegroundThemeBrush}"/>
                                <Setter Property="Background" Value="{ThemeResource TextBoxOptionBackgroundThemeBrush}"/>
                                <Setter Property="BorderBrush" Value="{ThemeResource OptionsBorderBrush}"/>
                                <Setter Property="BorderThickness" Value="{ThemeResource OptionControlBorderThemeThickness}"/>
                                <Setter Property="FontFamily" Value="{ThemeResource MainFontFamily}"/>
                                <Setter Property="FontSize" Value="{ThemeResource OptionControlFontSize}"/>
                                <Setter Property="Padding" Value="{ThemeResource OptionControlThemePadding}"/>
                                <Setter Property="Margin" Value="{ThemeResource OptionControlMarginThemeThickness}"/>
                                <Setter Property="VerticalAlignment" Value="Top"/>
