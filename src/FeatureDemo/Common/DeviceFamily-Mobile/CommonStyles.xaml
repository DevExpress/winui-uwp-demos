                                                        <Rectangle x:Name="FocusVisualBlackHorizontal" IsHitTestVisible="False" Stroke="{ThemeResource SystemControlBackgroundAltHighBrush}" StrokeEndLineCap="Square" StrokeDashArray="1,1" Opacity="0" StrokeDashOffset="0.5" Grid.RowSpan="3" Grid.ColumnSpan="3" />
